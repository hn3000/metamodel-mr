{
  "name": "@hn3000/metaapi",
  "version": "0.6.4",
  "description": "api descriptions using @hn3000/metaapi",
  "repository": "git@github.com:hn3000/metaapi",
  "main": "out/src/export.js",
  "types": "out/src/export.d.ts",
  "files": [
    "src/**",
    "out/**",
    "test/petstore.json",
    "test/*.ts"
  ],
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "pretest": "tsc",
    "precover": "tsc",
    "prepare": "tsc",
    "test": "node out/test/allTests.js",
    "cover": "nyc node out/test/allTests.js"
  },
  "keywords": [],
  "author": "Harald Niesche",
  "license": "ISC",
  "dependencies": {
    "@hn3000/json-ref": "^1.1.0",
    "@hn3000/metamodel": "^1.8.1",
    "@hn3000/simpletemplate": "^0.10.1",
    "isomorphic-fetch": "^2.2.1",
    "swagger-schema-official": "^2.0.0-bab6bed"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@types/node": "^11.9.4",
    "@types/swagger-schema-official": "^2.0.14",
    "nyc": "^15.0.0",
    "tsunit.external": "=2.0.10",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "all": false,
    "cache": false,
    "excludeAfterRemap": false,
    "extension": [
      ".js",
      ".ts"
    ],
    "include": [
      "src/**",
      "out/src/**"
    ],
    "exclude": [
      "test/**",
      "out/test/**",
      "out/src/**.d.ts"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  }
}
