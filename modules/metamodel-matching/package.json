{
  "name": "@hn3000/metamodel-matching",
  "version": "0.9.2",
  "description": "Meta Model Type Matching logic for dynamic UI construction and type-dependent rendering.",
  "repository": {
    "type": "git",
    "url": "git@github.com:hn3000/metamodel-mr"
  },
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "pretest": "tsc",
    "test": "mocha out/test/allTests.js",
    "pretest-debug": "tsc",
    "prepare": "tsc",
    "test-debug": "mocha --inspect-brk out/test/allTests.js",
    "cover": "nyc npm test"
  },
  "main": "out/src/index.js",
  "typings": "out/src/index.d.ts",
  "keywords": [
    "model",
    "driven",
    "metamodel"
  ],
  "author": "Harald Niesche",
  "license": "MIT",
  "files": [
    "src/**",
    "out/**",
    "!out/node_modules/**",
    "test/*.ts"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "@hn3000/mocha-delayed-logging": "^1.0.4",
    "chai": "^4.3.4",
    "mocha": "^10.2.0",
    "nyc": "^15.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@hn3000/json-ref": "^1.1.0",
    "@hn3000/metamodel": "^1.9.2"
  },
  "optionalDependencies": {
    "isomorphic-fetch": "^3.0.0"
  },
  "nyc": {
    "all": true,
    "excludeAfterRemap": false,
    "extension": [
      ".js",
      ".ts"
    ],
    "include": [
      "out/**",
      "src/*.ts"
    ],
    "exclude": [
      "test/**",
      "out/test/**"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  }
}
